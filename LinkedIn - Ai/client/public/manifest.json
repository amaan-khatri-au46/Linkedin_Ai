{
  "manifest_version": 3,
  "name": "Profile Explorer",
  "version": "1.0.0",
  "description": "Discover and navigate through user profiles effortlessly with this powerful extension",
  "icons": {
    "128": "logo128.png",
    "48": "logo48.png",
    "16": "logo16.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "ProfileExplorer",
    "default_icon": {
      "128": "logo128.png",
      "48": "logo48.png",
      "16": "logo16.png"
    }
  },
  "permissions": [
    "tabs"
  ]
}
